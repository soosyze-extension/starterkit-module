{
    "starterkit.index": {
        "methode": "GET",
        "path": "starterkit/index",
        "uses": "Starterkit@index"
    },
    "starterkit.admin": {
        "methode": "GET",
        "path": "admin/starterkit",
        "uses": "Starterkit@admin"
    },
    "starterkit.show": {
        "methode": "GET",
        "path": "starterkit/:id",
        "uses": "Starterkit@show",
        "with": {
            ":id": "[0-9]+"
        }
    },
    "starterkit.create": {
        "methode": "GET",
        "path": "starterkit/item",
        "uses": "Starterkit@create"
    },
    "starterkit.store": {
        "methode": "POST",
        "path": "starterkit/item",
        "uses": "Starterkit@store"
    },
    "starterkit.edit": {
        "methode": "GET",
        "path": "starterkit/:id/edit",
        "uses": "Starterkit@edit",
        "with": {
            ":id": "[0-9]+"
        }
    },
    "starterkit.update": {
        "methode": "POST",
        "path": "starterkit/:id/edit",
        "uses": "Starterkit@update",
        "with": {
            ":id": "[0-9]+"
        }
    },
    "starterkit.delete": {
        "methode": "POST",
        "path": "starterkit/:id/delete",
        "uses": "Starterkit@delete",
        "with": {
            ":id": "[0-9]+"
        }
    }
}
